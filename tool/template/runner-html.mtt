<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html>
	<head>
		<meta charset="utf-8">
		<title>Massive Unit Test Runner</title>
		<script language='JavaScript' type='text/JavaScript' src="tests-complete.js"></script>
		<script language='JavaScript' type='text/JavaScript'>
			var MAX_TESTS = ::testCount::;
			var i = 0;
			
			function testComplete()
			{
				i++;
				
				if (typeof window.callPhantom === 'function') {
					window.callPhantom({ action: 'testComplete', count:i, maxTests:MAX_TESTS });
				}
				
				if (i === MAX_TESTS) {
					BrowserTestsCompleteReporter.sendReport(shutdown, reportError);
				}
			}
			
			function reportError(){
				console.log("reportError - problem sending reports");
				shutdown();
			}
			
			function shutdown()
			{
				if (typeof window.callPhantom === 'function') {
					window.callPhantom({ action: 'shutdown' });
				}
				
				::if killBrowser::
					window.open('', '_self', '');
					window.close();
				::end::
			}
		</script>
		
		<style type="text/css">
			html, body, div { margin:0; padding:0; height:100%; background:#eee;}
		</style>
		
	</head>
	<frameset rows="83,*" border="1" bordercolor="#ccc">
		<frame src="target_headers.html"/>
		<frameset cols="::frameCols::" border="1">
			::frames::
		</frameset>
	</frameset>
</html>
